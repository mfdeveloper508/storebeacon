    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Aisle Tables
      </h1>
      <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Aisle Tables</a></li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <br/>
      <br/>
      <div class="row">
        <div class="col-md-1"> </div>
        <div class="col-md-10">
          <div class="box">
            <div class="box-header">
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <a href="/aisle/add/" class="btn btn-info">Add Aisle</a>
              <br>
              <br>
              <table id="aisle_data_table" class="table table-bordered table-striped" style="table-layout:fixed;">
                <thead>
                <tr>
                  <th>Id</th>
                  <th>Name</th>
                  <th>Description</th>
                  <th>Store Name</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                {{range $i, $elm :=.Aisles}}
                <tr>
                  <td>{{$elm.ID}}</td>
                  <td>{{$elm.Name}}</td>
                  <td>{{$elm.Description}}</td>
                  <td>{{$elm.Store.Title}}</td>
                  <td>
                    <button type="button" class="btn btn-info fa fa-edit" onclick="javascript:goEditFunction('/aisle/edit/{{$elm.ID}}')"></button>
                    <button type="button" data-toggle="modal" data-target="#modal-edit" class="btn btn-info fa fa-trash-o" onclick="goDeleteFunction('/aisle/remove/{{$elm.ID}}')"></button>
                  </td>
                </tr>
                {{end}}
                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

        <div class="modal modal-info fade" id="modal-edit">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Do you want to delete the Aisle?</h4>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-outline" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-outline" id="id_delete_submit" data-dismiss="modal">Ok</button>
              </div>
            </div>
            <!-- /.modal-content -->
          </div>
          <!-- /.modal-dialog -->
        </div>
        <!-- /.modal -->
    </section>